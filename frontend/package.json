{
  "name": "frontproject-development-serviceapi",
  "version": "1.0.0",
  "description": "Orange-themed React + Vite + TypeScript frontend with mobile-optimized design",
  "main": "index.js",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "coverage": "vitest run --coverage",
    "test:ci": "vitest run --coverage",
    "open": "start http://localhost:5173/",
    "preview:open": "vite preview --open",
    "open:live": "start https://hyukiody.github.io/frontproject-development-serviceApi/",
    "status:deploy": "start https://github.com/hyukiody/frontproject-development-serviceApi/actions/workflows/deploy.yml",
    "status:ci": "start https://github.com/hyukiody/frontproject-development-serviceApi/actions/workflows/ci.yml"
  },
  "keywords": [
    "deployment",
    "frontend",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@tanstack/react-query": "^5.90.16",
    "@types/qrcode": "^1.5.6",
    "date-fns": "^4.1.0",
    "i18next": "^25.7.3",
    "lucide-react": "^0.562.0",
    "qrcode": "^1.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^16.5.0",
    "react-router-dom": "^6.27.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.1.9",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eventsource": "^4.1.0",
    "globals": "^15.14.0",
    "jsdom": "^25.0.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.18.2",
    "vite": "^5.2.0",
    "vitest": "^2.1.5"
  }
}
